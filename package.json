{
  "name": "TigersBack",
  "version": "1.0.0",
  "description": "Golf Course Ranking Web-App",
  "main": "index.js",
  "repository": "https://github.com/tommy-hall/TigersBack.git",
  "author": "Tom Hall",
  "license": "Apache Licence 2.0 ",
  "scripts": {
    "start": "webpack && node server",
    "dev": "node server & webpack --watch",
    "test": "jest --watchAll",
    "build": "webpack --config ./webpack.config.js",
    "knex": "knex --knexfile ./server/db/knexfile.js"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "jest": {
    "setupFiles": [
      "./tests/setup.js"
    ]
  },
  "dependencies": {
    "express": "^4.16.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^22.1.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  }
}
